  "Default Button": {
    "prefix": "rsb",
    "body": ["<Button appearance=\"default\">$1</Button>", ""]
  },
  "Primary Button": {
    "prefix": "rsbp",
    "body": ["<Button appearance=\"primary\">$1</Button>", ""]
